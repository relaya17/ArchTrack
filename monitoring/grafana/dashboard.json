{
    "dashboard": {
        "id": null,
        "title": "ProBuilder - Construction Master App",
        "tags": [
            "probuilder",
            "construction",
            "monitoring"
        ],
        "style": "dark",
        "timezone": "browser",
        "refresh": "30s",
        "time": {
            "from": "now-1h",
            "to": "now"
        },
        "panels": [
            {
                "id": 1,
                "title": "HTTP Request Rate",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(http_requests_total[5m])",
                        "legendFormat": "{{method}} {{route}}"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "reqps",
                        "color": {
                            "mode": "palette-classic"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "Response Time",
                "type": "stat",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
                        "legendFormat": "95th percentile"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "s",
                        "color": {
                            "mode": "palette-classic"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 0
                }
            },
            {
                "id": 3,
                "title": "Error Rate",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(http_request_errors_total[5m])",
                        "legendFormat": "{{error_type}}"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "reqps",
                        "color": {
                            "mode": "palette-classic"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 8
                }
            },
            {
                "id": 4,
                "title": "Active Connections",
                "type": "stat",
                "targets": [
                    {
                        "expr": "active_connections",
                        "legendFormat": "Active"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "short",
                        "color": {
                            "mode": "palette-classic"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 8
                }
            },
            {
                "id": 5,
                "title": "Memory Usage",
                "type": "stat",
                "targets": [
                    {
                        "expr": "memory_usage_bytes{type=\"used\"}",
                        "legendFormat": "Used"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "bytes",
                        "color": {
                            "mode": "palette-classic"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 16
                }
            },
            {
                "id": 6,
                "title": "CPU Usage",
                "type": "stat",
                "targets": [
                    {
                        "expr": "cpu_usage_percent",
                        "legendFormat": "CPU %"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "percent",
                        "color": {
                            "mode": "palette-classic"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 16
                }
            },
            {
                "id": 7,
                "title": "Business Metrics",
                "type": "stat",
                "targets": [
                    {
                        "expr": "users_registered_total",
                        "legendFormat": "Users Registered"
                    },
                    {
                        "expr": "projects_created_total",
                        "legendFormat": "Projects Created"
                    },
                    {
                        "expr": "documents_generated_total",
                        "legendFormat": "Documents Generated"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "short",
                        "color": {
                            "mode": "palette-classic"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 24,
                    "x": 0,
                    "y": 24
                }
            },
            {
                "id": 8,
                "title": "Security Metrics",
                "type": "stat",
                "targets": [
                    {
                        "expr": "failed_logins_total",
                        "legendFormat": "Failed Logins"
                    },
                    {
                        "expr": "suspicious_activity_total",
                        "legendFormat": "Suspicious Activity"
                    },
                    {
                        "expr": "rate_limit_hits_total",
                        "legendFormat": "Rate Limit Hits"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "short",
                        "color": {
                            "mode": "palette-classic"
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 24,
                    "x": 0,
                    "y": 32
                }
            }
        ]
    }
}
