{
    "version": 2,
    "builds": [
        {
            "src": "apps/client/package.json",
            "use": "@vercel/next",
            "config": {
                "distDir": ".next"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apps/client/$1"
        }
    ]
}